<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <link rel="shortcut icon" href="images/favicon.png">

    <title>[ PhoneCell ] {% block title %}Bem vindo{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('bundles/basebase/bs3/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/css/bootstrap-reset.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/font-awesome/css/font-awesome.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/css/bootstrap-switch.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/bootstrap-fileupload/bootstrap-fileupload.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/bootstrap-datepicker/css/datepicker.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/jquery-multi-select/css/multi-select.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/jquery-tags-input/jquery.tagsinput.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/select2/select2.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/DataTables-1.10.5/media/css/dataTables.responsive.nightly.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/js/DataTables-1.10.5/media/css/jquery.dataTables.min.css') }}" rel="stylesheet"/>

        <link href="{{ asset('bundles/basebase/css/style.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/css/style-responsive.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/basebase/css/jquery.steps.css?1') }}" rel="stylesheet"/>
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('bundles/basebase/js/jquery.js') }}"></script>
        <script src="{{ asset('bundles/basebase/js/jquery-1.8.3.min.js') }}"></script>
        <script src="{{ asset('bundles/basebase/bs3/js/bootstrap.min.js') }}"></script>

        <script type="text/javascript" src="{{ asset('bundles/basebase/js/ie8-responsive-file-warning.js') }}"></script>
    {% endblock %}

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script type="text/javascript" src="{{ asset('bundles/basebase/js/html5shiv.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/basebase/js/respond.min.js') }}"></script>
    <![endif]-->
</head>

<body>

<section id="container" class="">
    <!--header start-->
    <header class="header fixed-top clearfix">
        <!--logo start-->
        <div class="brand">

            <a href="/" class="logo">
                Phone Cell
            {#de    <img src="{{ asset('bundles/basebase/images/logo.png') }}" alt="">#}
            </a>

            <div class="sidebar-toggle-box">
                <div class="fa fa-bars"></div>
            </div>
        </div>
        <!--logo end-->

        <div class="nav notify-row" id="top_menu">
            <!--  notification start -->
            <ul class="nav top-menu">
                <!-- settings start -->

                <!-- settings end -->
                <!-- inbox dropdown start-->

                <!-- inbox dropdown end -->
                <!-- notification dropdown start-->
                <li id="header_notification_bar" class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">

                        <i class="fa fa-bell-o"></i>
                        <span class="badge bg-warning">3</span>
                    </a>
                    <ul class="dropdown-menu extended notification">
                        <li>
                            <p>Notifications</p>
                        </li>
                        <li>
                            <div class="alert alert-info clearfix">
                                <span class="alert-icon"><i class="fa fa-bolt"></i></span>

                                <div class="noti-info">
                                    <a href="#"> Server #1 overloaded.</a>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="alert alert-danger clearfix">
                                <span class="alert-icon"><i class="fa fa-bolt"></i></span>

                                <div class="noti-info">
                                    <a href="#"> Server #2 overloaded.</a>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="alert alert-success clearfix">
                                <span class="alert-icon"><i class="fa fa-bolt"></i></span>

                                <div class="noti-info">
                                    <a href="#"> Server #3 overloaded.</a>
                                </div>
                            </div>
                        </li>

                    </ul>
                </li>
                <!-- notification dropdown end -->
            </ul>
            <!--  notification end -->
        </div>
        <div class="top-nav clearfix">
            <!--search & user info start-->
            <ul class="nav pull-right top-menu">

                <!-- user login dropdown start-->
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                        <img alt="" src="{{ asset('bundles/basebase/images/avatar1_small.jpg') }}">
                        <span class="username">{{ app.security.getToken().getUser().getIdPessoa().getNoPessoa(true) }}</span>
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu extended logout">
                        <li><a href="#"><i class=" fa fa-suitcase"></i>Profile</a></li>
                        <li><a href="#"><i class="fa fa-cog"></i> Settings</a></li>
                        <li><a href="{{ path('usuario_logout') }}"><i class="fa fa-key"></i> Sair</a></li>
                    </ul>
                </li>
                <!-- user login dropdown end -->

            </ul>
            <!--search & user info end-->
        </div>
    </header>
    <!--header end-->
    <aside>
        <div id="sidebar" class="nav-collapse">
            <!-- sidebar menu start-->
            <div class="leftside-navigation" tabindex="5000" style="overflow: hidden; outline: none;">
                {% include 'BaseBaseBundle::menu.html.twig' %}
            </div>
            <!-- sidebar menu end-->
            <div id="ascrail2000" class="nicescroll-rails"
                 style="width: 3px; z-index: auto; cursor: default; position: absolute; top: 0px; left: 237px; height: 446px; opacity: 1; display: block;">
                <div style="position: relative; top: 0px; float: right; width: 3px; height: 185px; border: 0px solid rgb(255, 255, 255); border-radius: 0px; background-color: rgb(31, 181, 173); background-clip: padding-box;"></div>
            </div>
            <div id="ascrail2000-hr" class="nicescroll-rails"
                 style="height: 3px; z-index: auto; top: 443px; left: 0px; position: absolute; cursor: default; display: none; width: 237px; opacity: 1;">
                <div style="position: relative; top: 0px; height: 3px; width: 240px; border: 0px solid rgb(255, 255, 255); border-radius: 0px; background-color: rgb(31, 181, 173); background-clip: padding-box;"></div>
            </div>
        </div>
    </aside>
    <!--sidebar end-->
    <!--main content start-->
    <section id="main-content" class="">
        <section class="wrapper">
            {% if(app.session.flashbag.peekAll|length) %}
                {% for label, flashes in app.session.flashbag.all %}
                    <div class="alert alert-block alert-{{ label|replace({'error': 'danger'}) }} fade in">
                        <button type="button" class="close close-sm" data-dismiss="alert">
                            <i class="fa fa-times"></i>
                        </button>

                        {% for flash in flashes %}
                            {{ flash }}<br/>
                        {% endfor %}
                    </div>
                {% endfor %}
            {% endif %}

            <!-- page start-->
            {% block body %}
            {% endblock %}
            <!-- page end-->
        </section>
    </section>
    <!--main content end-->
    <!--right sidebar start-->
</section>

{% block javascript %}
    <script src="{{ asset('bundles/basebase/js/jquery-ui-1.9.2.custom.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery.validate.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery.dcjqaccordion.2.7.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery.scrollTo.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jQuery-slimScroll-1.3.0/jquery.slimscroll.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery.nicescroll.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/bootstrap-switch.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/fuelux/js/spinner.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/bootstrap-fileupload/bootstrap-fileupload.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/bootstrap-datepicker/js/bootstrap-datepicker.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery-multi-select/js/jquery.multi-select.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery-multi-select/js/jquery.quicksearch.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/bootstrap-inputmask/bootstrap-inputmask.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery-tags-input/jquery.tagsinput.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/select2/select2.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/select-init.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/jquery-steps/jquery.steps.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/DataTables-1.10.5/media/js/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/DataTables-1.10.5/media/js/dataTables.responsive.nightly.min.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/sparkline/jquery.sparkline.js') }}"></script>
    <script src="{{ asset('bundles/basebase/js/scripts.js') }}"></script>

    <script src="{{ asset('bundles/basebase/app/grid.js') }}"></script>
    <script src="{{ asset('bundles/basebase/app/validate.js') }}"></script>
{% endblock %}

</body>
</html>